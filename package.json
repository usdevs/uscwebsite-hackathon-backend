{
  "name": "usc-backend",
  "version": "1.0.0",
  "description": "USC backend ptototype",
  "main": "src/app.js",
  "scripts": {
    "test": "jest --coverage",
    "build": "swc src -d dist --source-maps --copy-files",
    "start": "npm run build && cross-env NODE_ENV=production node dist/app.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "lint": "eslint --ignore-path .gitignore src/**/*.{js,ts,json}",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write 'src/**/*.{js,ts,md,json}' --config ./.prettierrc.json",
    "prisma:init": "prisma init",
    "prisma:migrate": "prisma migrate dev --preview-feature",
    "prisma:seed": "prisma db seed",
    "prisma:generate": "prisma generate"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@faker-js/faker": "^7.6.0",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.22",
    "@types/cors": "^2.8.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.13",
    "babel-jest": "^29.3.1",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "jest-mock-extended": "^2.0.4",
    "nodemon": "^2.0.20",
    "prettier": "2.8.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@prisma/client": "^4.8.1",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.4",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "nvm": "^0.0.4",
    "prisma": "^4.7.1",
    "zod": "^3.20.2"
  }
}
