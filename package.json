{
  "name": "usc-backend",
  "version": "1.0.0",
  "description": "USC backend ptototype",
  "main": "src/app.js",
  "scripts": {
    "test:unit": "jest -c ./jest.config.js --coverage",
    "build": "npm run prisma:generate && tsc",
    "start": "npm run build && node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/src/app.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "lint": "eslint --ignore-path .gitignore src/**/*.{js,ts,json}",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write 'src/**/*.{js,ts,md,json}' --config ./.prettierrc.json",
    "prisma:init": "prisma init",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "prisma db seed",
    "prisma:generate": "prisma generate",
    "prisma:reset": "prisma migrate reset"
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register --transpile-only prisma/seed.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@faker-js/faker": "^7.6.0",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.22",
    "@types/cors": "^2.8.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.13",
    "babel-jest": "^29.3.1",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "jest-mock-extended": "^2.0.4",
    "nodemon": "^2.0.20",
    "prettier": "2.8.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@prisma/client": "^4.7.1",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.4",
    "@types/morgan": "^1.9.9",
    "body-parser": "^1.20.1",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "nvm": "^0.0.4",
    "prisma": "^4.7.1",
    "read-excel-file": "^5.6.1",
    "slugify": "^1.6.6",
    "zod": "^3.22.3"
  }
}
